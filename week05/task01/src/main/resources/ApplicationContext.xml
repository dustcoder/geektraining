<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user1" class="com.ren.task01.way01.User">
        <constructor-arg index="0" value="jack"></constructor-arg>
        <constructor-arg index="1" value="123"></constructor-arg>
    </bean>
    <bean id="user2" class="com.ren.task01.way01.User">
        <property name="userName" value="rose"/>
        <property name="password" value="1234556"/>
    </bean>
</beans>